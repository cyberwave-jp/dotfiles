[color]
  ui = auto
[color "status"]
  added = yellow
  chenged = green
  untracked = cyan
[core]
  excludesfile = ~/.gitignore_global
  attributefile = ~/.gitattributes_global
  autocrlf = false
	editor = vim
	quotepath = false
	pager = nkf -w | LESSCHARSET=utf-8 less
[alias]
  st = status
  co = commit
  ch = checkout
  br = branch
  df = diff --ignore-space-at-eol --color-words
  dc = diff --cached
  lg = log -p
  lol = log --graph --decorate --pretty=oneline --abbr
  lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
  ls = ls-files
  sh = show-branch
  today = "!sh -c 'git log --reverse --no-merges --branches=* --date=local --since=midnight --oneline --author=\"`git config --get user.name`\"'"
[push]
	default = simple
[mergetool]
	keepBackup = false
